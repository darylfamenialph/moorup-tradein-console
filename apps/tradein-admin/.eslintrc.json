{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json", "plugin:prettier/recommended"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "eol-last": ["error", "always"],
        "quotes": ["error", "single"]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
